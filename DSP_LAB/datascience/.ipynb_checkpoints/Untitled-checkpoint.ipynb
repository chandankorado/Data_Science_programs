   "source": [
    "# swapping file\n",
    "\n",
    "f1 = open(\"file1.txt\",\"r\")\n",
    "f2 = open(\"file2.txt\",\"r\")\n",
    "\n",
    "t1 = f1.read()\n",
    "t2 = f2.read()\n",
    "\n",
    "f1.close()\n",
    "f2.close()\n",
    "\n",
    "f1 = open(\"file1.txt\",\"w\")\n",
    "f2 = open(\"file2.txt\",\"w\")\n",
    "\n",
    "f1.write(t2)\n",
    "f2.write(t1)\n",
    "\n",
    "f1.close()\n",
    "f2.close()"
   ]
  }
 ],
